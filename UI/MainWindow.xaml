        <TextBox x:Name="TXT_TARGET" HorizontalAlignment="Left" Height="23" Margin="295,90,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120" RenderTransformOrigin="0.208,0.435"/>
        <Button x:Name="BTN_TARGET" Content="..." HorizontalAlignment="Left" Margin="420,90,0,0" VerticalAlignment="Top" Width="20" Height="23" RenderTransformOrigin="8.5,2.565" Click="BTN_TARGET_Click"/>

    </Grid>
</Window>
