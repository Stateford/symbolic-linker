        <TextBox x:Name="TXT_PATH" HorizontalAlignment="Left" Height="23" Margin="110,90,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120" RenderTransformOrigin="0.208,0.435"/>
